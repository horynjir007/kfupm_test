<launch>

    <arg name="UAV_NAME" default="$(optenv UAV_NAME)" />

    <arg name="fcu_frame" default="$(arg UAV_NAME)/fcu" />

    <group ns="$(arg UAV_NAME)">

        <!-- TF -->
        <node pkg="tf2_ros" type="static_transform_publisher" name="oak_tf" args="0.165 0.0 -0.086 0.0 0.0 3.14159265359 $(arg fcu_frame) oak-d-base-frame" />
        
    </group>

</launch>
